<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RandomInt">
        <attr name="myInt" format="string" />
        <attr name="myBgColor" format="color" />
        <attr name="myTextColor" format="color" />
        <attr name="myTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="Myv">
        <attr name="mytitleText" format="string" />
        <attr name="mytitleTextColor" format="color" />
        <attr name="mytitleTextSize" format="dimension" />
        <attr name="mytitleTextBg" format="color" />
    </declare-styleable>
    <declare-styleable name="BiuEditStyle">
        <attr name="biu_text_color" format="color" />
        <attr name="biu_text_start_size" format="dimension" />
        <attr name="biu_text_scale" format="float" />
        <attr name="biu_duration" format="integer" />
        <attr name="biu_type" format="enum">
            <enum name="flyup" value="0" />
            <enum name="flydown" value="1" />
        </attr>
    </declare-styleable>

</resources>